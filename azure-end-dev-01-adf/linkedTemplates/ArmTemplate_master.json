						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_tenant')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_servicePrincipalId')]"
					},
					"AzureSqlDatabasefor internaldb_source_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabasefor internaldb_source_properties_typeProperties_server')]"
					},
					"AzureSqlDatabasefor internaldb_source_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabasefor internaldb_source_properties_typeProperties_database')]"
					},
					"AzureSqlDatabasefor internaldb_source_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabasefor internaldb_source_properties_typeProperties_userName')]"
					}
				}
