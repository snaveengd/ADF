		"AzureSqlDatabasefor internaldb_source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Azure sql"
		},
		"AzureSqlDatabasefor internaldb_source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "azuredev01"
		},
		"RestService_source_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airlabs.co/api/v9/airlines"
		},
		"containerUri": {
